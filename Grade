#include<stdio.h>
void main()
{
float marks;
printf("\n Enter mark of student in 100:");
scanf("%f",&marks);
if(marks>=85 && marks<=100)
printf("Grade A");
else if(marks>=70 && marks<=84)
printf(" Grade B");
else if(marks>=55 && marks<=69)
printf(" Grade C");
else if(marks>=40 && marks<=54)
printf(" Grade D");
else if(marks>=51 && marks<=60)
printf(" Grade C");
else if(marks<40)
printf(" Grade F");
else 
printf("Invalid Input");
}
